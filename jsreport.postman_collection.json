{
    "info": {
      "name": "jsreport API",
      "_postman_id": "jsreport-collection",
      "description": "Generate report from pre-created template",
      "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
    },
    "item": [
      {
        "name": "Generate Report",
        "request": {
          "method": "POST",
          "header": [
            { "key": "Content-Type", "value": "application/json" }
          ],
          "body": {
            "mode": "raw",
            "raw": "{\n  \"template\": { \"name\": \"Test Template\" },\n  \"data\": { \"foo\": \"World\" }\n}"
          },
          "url": {
            "raw": "http://localhost:5488/api/report",
            "protocol": "http",
            "host": ["localhost"],
            "port": "5488",
            "path": ["api", "report"]
          }
        }
      }
    ]
  }
  